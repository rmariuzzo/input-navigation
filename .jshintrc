{
    "bitwise": true,
    "camelcase": true,
    "curly": false,
    "eqeqeq": true,
    "forin": true,
    "immed": false,
    "indent": 4,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "nonew": true,
    "quotmark": true,
    "undef": true,
    "unused": true,
    "trailing": true,
    "node": true,
    "strict": true,
    "predef": ["_und", "require", "describe", "beforeEach", "it"],
    "globals": {
        "it": false,
        "xit": false,
        "describe": false,
        "xdescribe": false,
        "beforeEach": false,
        "afterEach": false,
        "expect": false,
        "spyOn": false
    }
}
